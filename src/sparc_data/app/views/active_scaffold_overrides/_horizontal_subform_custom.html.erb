<script>
	$('.destroy').on('click', function(e) {
		console.log($(this).closest('table').remove());
		e.preventDefault();
	})
</script>
<table id="<%= sub_form_list_id(:association => column.name, :id => parent_record.try(:id) || generated_id(parent_record) || 99999999999) %>">
<%
  header_record_class = column.association.klass
-%>
  <%= render :partial => 'horizontal_subform_header', :locals => {:parent_record => parent_record, :record_class => header_record_class} %>

  <%= render :partial => 'form_association_record', :object => object, :locals => { :parent_record => parent_record, :column => column} %>
  <tfoot>
    <%= render :partial => 'horizontal_subform_footer', :locals => { :parent_record => parent_record, :column => column} %>
  </tfoot>
</table>